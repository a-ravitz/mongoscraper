<div id="notes"></div>

<div class="background"></div>


{{#each saved}}

<div class="container m-2 ">
 <div class="card border-dark mb-3 row1" style="max-width: 800px" data-id={{this.id}}>  
     <div class="card-header">
    <h3>{{this.title}}</h3>
  </div>
  <div class="card-body d-flex">
    <div><h5 class="card-title">{{this.summary}}</h5>
    <button class="delete" data-id={{this.id}}>Delete This Article</button>
</div>
    <a href={{this.link}}><img src={{this.image}} class="rounded float-right size" alt="..."></a>
  </div>
  
</div>

</div>
{{/each}}

